# ===================================
# NÍVEL 2: DEVELOPMENT ENVIRONMENT
# ===================================
# Configuração para ambiente de desenvolvimento
# Senhas mais seguras mas ainda legíveis para debug

# Environment Configuration
ENVIRONMENT=development
DEPLOY_TIMESTAMP=$(date '+%Y%m%d_%H%M%S')

# MySQL Configuration
MYSQL_VERSION=8.3
MYSQL_DATABASE=zabbix
MYSQL_USER=zabbix
MYSQL_PASSWORD=Dev_Zabbix_2024!@
MYSQL_ROOT_PASSWORD=DevRoot_Secure_2024!@

# Additional MySQL users for security
MYSQL_EXPORTER_USER=exporter
MYSQL_EXPORTER_PASSWORD=DevExporter_2024!@

# Zabbix Configuration  
ZABBIX_VERSION=alpine-7.0.5
DB_SERVER_HOST=127.0.0.1
ZBX_SERVER_HOST=127.0.0.1
PHP_TZ=America/Sao_Paulo
TZ=America/Sao_Paulo

# Zabbix Performance (Development - lower values)
ZBX_STARTPOLLERS=3
ZBX_STARTPINGERS=1
ZBX_STARTUNREACHABLE=1
ZBX_STARTSNMPPOLLERS=5
ZBX_STARTTRAPPERS=2
ZBX_STARTDBSYNCERS=2
ZBX_TIMEOUT=30
ZBX_VALUECACHESIZE=128M
ZBX_HISTORYCACHESIZE=64M
ZBX_HISTORYINDEXCACHESIZE=32M
ZBX_CACHESIZE=256M
ZBX_TRENDCACHESIZE=64M

# Grafana Configuration
GRAFANA_VERSION=12.0.2-security-01-ubuntu
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=DevAdmin_2024!@

# Prometheus Configuration
PROMETHEUS_VERSION=latest

# Security Settings (Development)
ENABLE_DEBUG_LOGS=true
DISABLE_SSL_VERIFICATION=true  
ALLOW_WEAK_PASSWORDS=false

# Backup & Retention (Development)
BACKUP_RETENTION_DAYS=7
LOG_RETENTION_DAYS=3